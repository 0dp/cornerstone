{
  "name": "Stencil",
  "version": "1.0",
  "css_compiler": "scss",
  "autoprefixer_cascade": true,
  "autoprefixer_browsers": ["> 5% in US"],
  "variations": [
    {
      "name": "Bold",
      "settings": {
        "homepage-numOfNewProducts": 8,
        "homepage-numOfFeaturedProducts": 8,
        "homepage-numOfTopProducts": 8,
        "homepage-showCarousel": true,
        "color-primary": "#4f4f4f",
        "color-primaryDark": "#454545",
        "color-primaryLight": "#a5a5a5"
      },
      "images": {
        "logo": {
          "width": 250,
          "height": 100
        },
        "thumb": {
          "width": 100,
          "height": 100
        },
        "product": {
          "width": 500,
          "height": 659
        },
        "gallery": {
          "width": 190,
          "height": 250
        },
        "zoom": {
          "width": 1280,
          "height": 1280
        }
      }
    },
    {
      "name": "Light",
      "settings": {
        "homepage-numOfNewProducts": 4,
        "homepage-numOfFeaturedProducts": 4,
        "homepage-numOfTopProducts": 4,
        "homepage-showCarousel": false,
        "color-primary": "#4f4f4f",
        "color-primaryDark": "#454545",
        "color-primaryLight": "#a5a5a5"
      },
      "images": {
        "logo": {
          "width": 250,
          "height": 100
        },
        "thumb": {
          "width": 100,
          "height": 100
        },
        "product": {
          "width": 500,
          "height": 659
        },
        "gallery": {
          "width": 190,
          "height": 250
        },
        "zoom": {
          "width": 1280,
          "height": 1280
        }
      }
    }
  ]
}
