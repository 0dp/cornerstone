{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-dangle": [
            2,
            "never"
        ],
        "no-use-before-define": [
            2,
            "nofunc"
        ],
        "no-alert": 0,
        "no-param-reassign": 0,
        "no-shadow": 0,
        "func-names": 0,
        "consistent-return": 0
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "airbnb/base"
}
